<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="558.0" prefWidth="697.0" styleClass="card" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SeatSelectController">
   <top>
      <FlowPane alignment="CENTER" prefHeight="69.0" prefWidth="445.0" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="textFilmDetails" strokeType="OUTSIDE" strokeWidth="0.0" text="Film" textAlignment="CENTER" wrappingWidth="444.0078125" />
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="9.0" stroke="BLACK" strokeType="INSIDE" width="411.0">
               <FlowPane.margin>
                  <Insets top="20.0" />
               </FlowPane.margin>
            </Rectangle>
         </children>
      </FlowPane>
   </top>
   <bottom>
      <FlowPane alignment="CENTER_RIGHT" prefHeight="42.0" prefWidth="445.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#doClose" prefHeight="40.0" prefWidth="78.0" styleClass="button-flat" text="%seat_back">
               <FlowPane.margin>
                  <Insets right="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="buttonOk" mnemonicParsing="false" onAction="#doContinue" prefHeight="40.0" prefWidth="73.0" styleClass="button-flat" text="%seat_next">
               <FlowPane.margin>
                  <Insets right="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </bottom>
   <center>
      <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane fx:id="gridSeats" alignment="CENTER">
      </GridPane>
         </children>
      </FlowPane>
   </center>
</BorderPane>
