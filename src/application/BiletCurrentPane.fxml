                <TableColumn fx:id="columnTitle" prefWidth="118.0" text="%ticket_film" />
                <TableColumn fx:id="columnTyp" prefWidth="94.0" text="%ticket_type" />
                  <TableColumn fx:id="columnData" prefWidth="120.0" text="%ticket_date" />
                  <TableColumn fx:id="columnKino" prefWidth="51.0" text="%ticket_cinema" />
                  <TableColumn fx:id="columnSala" minWidth="0.0" prefWidth="51.0" text="%ticket_sala" />
