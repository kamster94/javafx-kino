            <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="card-subtitle" text="%currticket_title" />
         </children>
      </FlowPane>
   </top>
   <center>
      <FlowPane prefHeight="322.0" prefWidth="475.0" BorderPane.alignment="CENTER">
